  Author                   = {Weinstein, J. N. and Myers, T. G. and O'Connor, P. M. and Friend, S. H. and Fornace, Jr, AJ and Kohn, K. W. and Fojo, T. and Bates, S. E. and Rubinstein, L. V. and Anderson, N. L. and Buolamwini, J. K. and {van Osdol}, W. W. and Monks, A. P. and Scudiero, D. A. and Sausville, E. A. and Zaharevitz, D. W. and Bunow, B. and Viswanadhan, V. N. and Johnson, G. S. and Wittes, R. E. and Paull, K. D.},
  Journal                  = {Science},
  Year                     = {1997},

