@Article{xhd+08,
  Title                    = {Cellular imaging predictions of clinical drug-induced liver injury.},
  Author                   = {Xu, Jinghai J. and Henstock, Peter V. and Dunn, Margaret C. and Smith, Arthur R. and Chabot, Jeffrey R. and {de Graaf}, David},
  Journal                  = {Toxicol Sci},
