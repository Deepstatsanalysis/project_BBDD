@Article{nbjm08,
  Title                    = {Ligand-target prediction using Winnow and naive Bayesian algorithms and the implications of overall performance statistics.},
  Author                   = {Nigsch, Florian and Bender, Andreas and Jenkins, Jeremy L. and Mitchell, John B O.},
  Journal                  = {J Chem Inf Model},
