@Article{cbt+14,
  Title                    = {Toward predictive models for drug-induced liver injury in humans: are we there yet?},
  Author                   = {Chen, Minjun and Bisgin, Halil and Tong, Lillian and Hong, Huixiao and Fang, Hong and Borlak, J{\"{u}}rgen and Tong, Weida},
