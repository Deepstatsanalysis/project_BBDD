@Article{bbt+12,
  Title                    = {The new ISSMIC database on in vivo micronucleus and its role in assessing genotoxicity testing strategies.},
  Author                   = {Benigni, Romualdo and Bossa, Cecilia and Tcheremenskaia, Olga and Battistelli, Chiara Laura and Crettaz, Pierre},
  Journal                  = {Mutagenesis},
