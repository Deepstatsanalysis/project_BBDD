  Institution              = {Department of Pharmaceutical Chemistry, University of California San Francisco, San Francisco, CA 94158-2330, USA.},
  Keywords                 = {Benchmarking; Cluster Analysis; Drug Discovery, methods/standards; Humans; Ligands; Models, Molecular; Protein Binding; Protein Conformation; Proteins, chemistry/metabolism; Reference Standards},
  Language                 = {eng},
